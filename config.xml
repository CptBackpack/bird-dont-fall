<?xml version='1.0' encoding='utf-8'?>
<widget id="com.birddontfall.apk" version="0.1.1-proto" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bird, Don't Fall!</name>
    <description>
        Bird, Don't Fall! is a game in which you have to take care of your pet parrot. 
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Sgt.Backpack
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-browsersync" spec="^1.1.0" />
    <engine name="android" spec="^8.0.0" />
    <engine name="browser" spec="^6.0.0" />
</widget>
